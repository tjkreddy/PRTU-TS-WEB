{
  "name": "prtu-ts-live-portal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "compile-server": "npx tsc -p tsconfig.server.json",
    "server": "node dist/server.js",
    "dev:server": "npx ts-node --project tsconfig.server.json server/server.ts",
    "dev:server-js": "node server-js.js",
    "start:full": "concurrently \"npm run dev\" \"npm run server\"",
    "start:dev": "npm run compile-server && concurrently \"npm run dev\" \"npm run server\"",
    "start:simple": "concurrently \"npm run dev\" \"node server-js.js\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.1.0",
    "express": "^5.1.0",
    "mongodb": "^6.17.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.1",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vite-plugin-pwa": "^1.0.1",
    "workbox-window": "^7.3.0"
  }
}
